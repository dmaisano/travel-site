/* Handlebars code for generating the necessary css to use the sprite svg*/
/* To edit, go edit the sprite.hbs file located in dist/sprite.hbs */
{{#shapes}}
.icon--{{base}} {
	width: {{width.outer}}px;
	height: {{height.outer}}px;
	background-image: url('./sprite.svg');
	background-position: {{position.relative.xy}};
}
{{/shapes}}
