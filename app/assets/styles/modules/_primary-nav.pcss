.primary-nav {
  

  &--pull-right {
    float: right;
  }

  ul {
    margin: 0;
    padding: 0;
    @mixin clearFix;
  }

  li {
    list-style: none;
    float: left;
    padding-right: 1.25rem;
  }

  a {
    color: #fff;
    text-decoration: none;
    font-weight: 400;
    display: block;
    padding: 12px 0;
  }
}
